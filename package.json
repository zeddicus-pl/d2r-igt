{
  "name": "d2r-igt",
  "version": "1.0.0",
  "main": "dist/igt.cjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "parcel build --target=main igt.ts && parcel build --target=worker worker.ts && parcel build --target=cli cli.ts",
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm ./cli.ts"
  },
  "dependencies": {
    "@types/ffi-napi": "^4.0.5",
    "@types/ref-napi": "^3.0.4",
    "ffi-napi": "^4.0.3",
    "ref-napi": "^3.0.3",
    "ref-union-di": "^1.0.1",
    "win32-def": "^9.4.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.6.0",
    "@parcel/transformer-typescript-types": "2.6.0",
    "@types/chroma-js": "^2.1.3",
    "@types/ref-struct-di": "^1.1.6",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "os-browserify": "^0.3.0",
    "parcel": "^2.6.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "util": "^0.12.4"
  },
  "cli": "dist/cli.cjs",
  "worker": "dist/worker.cjs",
  "targets": {
    "main": {
      "engines": {
        "node": ">= 16"
      },
      "includeNodeModules": {
        "node-gyp-build": false
      },
      "optimize": false,
      "outputFormat": "commonjs"
    },
    "worker": {
      "engines": {
        "node": ">= 16"
      },
      "includeNodeModules": {
        "node-gyp-build": false
      },
      "optimize": false,
      "outputFormat": "commonjs"
    },
    "cli": {
      "engines": {
        "node": ">= 16"
      },
      "includeNodeModules": {
        "node-gyp-build": false
      },
      "optimize": false,
      "outputFormat": "commonjs"
    }
  }
}
